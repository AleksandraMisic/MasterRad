<UserControl x:Class="DNP3Outstation.View.ShellFillers.DevicesExplorer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DNP3Outstation.View.ShellFillers"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="{StaticResource ResourceKey=DarkColor}">
        <TreeView Background="Transparent" Margin="10, 20, 10, 10" TextBlock.FontSize="15" ItemsSource="{Binding Devices}">
        </TreeView>
    </Grid>
</UserControl>
